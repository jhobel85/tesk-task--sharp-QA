{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "shell",
      "args": ["build", "${workspaceFolder}/ReplicaTool.csproj"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
        {
      "label": "build_test",
      "command": "dotnet",
      "type": "shell",
      "args": ["build","${workspaceFolder}/tests/ReplicaTool.Tests.csproj"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
  ]
}
